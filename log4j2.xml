<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO" packages="net.minecraft,com.mojang">
	<Appenders>
		<Console name="SysOut" target="SYSTEM_OUT">
			<PatternLayout pattern="[%d{HH:mm:ss}] [%t/%level]: %msg%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="info">
			<AppenderRef ref="SysOut"/>
		</Root>
	</Loggers>
</Configuration>
