#!/bin/sh

set -x

BUILD_DIR=$1
CACHE_DIR=$2
ENV_DIR=$3

env
#uname -a
#ps aux
#ls -l $BUILD_DIR
#ls -l ansible
#ls -l ../ansible
find $BUILD_DIR -name "*ansible*"
ansible --version
ansible-playbook -vvvv $BUILD_DIR/ansible/local.ini $BUILD_DIR/ansible/local.yml
#du -h

echo $BUILD_DIR $CACHE_DIR $ENV_DIR

exit 0
